{
  "moduleFileExtensions": ["js", "json", "ts", "png"],
  "rootDir": "../",
  "testRegex": ".e2e-spec.ts|.unit.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "globalSetup": "<rootDir>/test/setup.ts",
  "globalTeardown": "<rootDir>/test/teardown.ts",
  "collectCoverageFrom": ["<rootDir>/src/**/*.(t)s"],
  "coverageDirectory": "<rootDir>/coverage",
  "testEnvironment": "node",
  "moduleNameMapper": {
    "^@/(.*)$": "<rootDir>/src/$1",
    "^Tests/(.*)$": "<rootDir>/test/$1"
  },
  "testTimeout": 120000
}
